.pubTile {
  display: flex;
  justify-content: space-between;
  height: 60rpx;
 /* border: 1px solid red;*/
  align-items: center;
  margin-bottom: 40rpx;
}

.pubTile .txt {
  font-family:‘Times New Roman’, Times, serif;
  font-size: 40 rpx;
  height: 100%;
  position: relative;
  padding-left: 20rpx;
}

.pubTile .txt:before {
  font-family:‘Times New Roman’, Times, serif;
  display: block;
  width: 10rpx;
  height: 30rpx;
 background:
    #7cc2f1;
  content: "";
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -25rpx;
}

.pubTile .more {
  font-family:‘Times New Roman’, Times, serif;
  font-size: 28rpx;
  color: #666;
  float: right;
}




.header {
  width: 750rpx;
  height: 90rpx;
