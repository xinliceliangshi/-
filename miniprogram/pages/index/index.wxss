  left: 0;
  opacity: 0;
}


.banner {
  width: 100%;
  height: 300rpx;
}

.banner swiper {
  height: 100%;
  width: 100%;
}

.banner image {
  width: 100%;
  height: 295rpx;
}

.liubai {
  width: 750rpx;
  height: 10rpx;
}

